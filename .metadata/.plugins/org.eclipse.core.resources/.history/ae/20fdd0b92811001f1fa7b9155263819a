package oop1;

public class Course {
	
	//constructor
	public Course(String title, int fee, int duration) {
		super();
		this.title = title;
		this.fee = fee;
		this.duration = duration;
	}

	// Instance variables
	private String title;
	private int fee, duration;

	public int netFee() {
		return fee+(fee*12/100);
	}
}
