package oop1;

public class Course {
	
	// Instance variables
	private String title;
	private int fee, duration, taxRate = 12;
	
	//constructor
	public Course(String title, int fee, int duration) {
		this.title = title;
		this.fee = fee;
		this.duration = duration;
	}

	public int netFee() {
		return this.fee+(this.fee*taxRate/100);
	}
	
	public int getTaxRate() {
		return this.taxRate;
	}
}
