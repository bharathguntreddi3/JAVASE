package oop1;

class Employee {
	protected String name, desg;
	public Employee(String name, String desg) {
		this.name = name;
		this.desg = desg;
	}
	public String getDesg() {
		return this.desg;
	}
	public void setDesg(String desg) {
		this.desg = desg;
	}
	public String getName() {
		return this.name = name;
	}
	public abstract int getPay();
}

