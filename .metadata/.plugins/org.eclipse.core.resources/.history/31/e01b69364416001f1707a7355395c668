//}

public class ReverseRandomArray {

	public static void main(String[] args) {
		
		int a[] = new int[10];
		
		for (int i = 1 ; i<=10; i++) {
//			System.out.println((int)(Math.random()*100));
			a[i] = a[i] + (int)(Math.random()*100);
		}
		System.out.println(a);
	}

}
